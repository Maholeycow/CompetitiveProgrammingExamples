 * Score: 226.09 out of 250 points
 */
 
#include <iostream>
#include <vector>
using namespace std;
class AccessLevel{
 string returnString;
 public:   
    AccessLevel()
    {
        returnString = {'\0'};   
    }
    
    string canAccess(vector<int> rights, int minPermission)
    {
        vector<int>::iterator myIterator = rights.begin();
        while(myIterator!=rights.end())
        {
         	if(*myIterator<minPermission)
                returnString+='D';
            else
                returnString+='A';
            myIterator++;
        }
        return returnString;
    }
};
